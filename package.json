  "betterScripts": {
    "start": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-storybook": {
      "command": "start-storybook -p 9001",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-test": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-dev-render-on-client": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "__DISABLE_SSR__": true
      }
    },
    "watch-client": {
      "command": "node ./bin/webpack.js",
      "env": {
        "UV_THREADPOOL_SIZE": 4,
        "NODE_PATH": "./src/"
      }
    },
    "build": {
      "command": "webpack --color --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "stats": {
      "command": "webpack --color --display-error-details --config webpack/prod.config.js --json > webpack-stats.json",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-storybook": {
      "command": "build-storybook -o static/__components",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "blueprints"
    ],
    "moduleNameMapper": {
      "^.+\\.styl": "identity-obj-proxy",
      "^.+\\.svg$": "<rootDir>/webpack/svgMock.js"
    },
    "transform": {
      "^.+\\.js*": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!d3-array|d3-scale|d3-shape|d3-time)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.story.js",
      "!**/*.mock.js",
      "!**/index.js",
      "!./webpack/**"
    ]
  },
  "moduleRoots": [
    "src"
  ]
}
