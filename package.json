    "redux-act": "^1.3.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-form": "^7.0.3",
    "redux-loop": "2.2.2",
    "remote-redux-devtools": "^0.5.12",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.2",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "url-loader": "^0.5.9",
