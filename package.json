    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-hot-middleware": "^2.18.2",
    "why-did-you-update": "^0.0.8"
  },
  "engines": {
    "node": "5.6.0"
  },
  "precommit.silent": true,
  "pre-commit": "lint-staged"
}
