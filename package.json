{
  "name": "4team",
  "description": "Big tasks store",
  "author": "Imater <eugene.leonar@gmail.com> (https://github.com/Imater)",
  "private": true,
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/imater/4team"
  },
  "homepage": "http://relef.ru/",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot",
    "react-hot-reloader",
    "redux",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "better-npm-run start",
    "start-test": "better-npm-run start-test",
    "build": "better-npm-run build",
    "stats": "better-npm-run stats & webpack-bundle-size-analyzer webpack-stats.json",
    "lint": "eslint -c .eslintrc src api --ext js,jsx",
    "lint-fix": "eslint -c .eslintrc src api --ext js,jsx --fix",
    "lint-errors": "eslint -c .eslintrc src api --quiet --ext js,jsx",
    "start-dev": "better-npm-run start-dev",
    "start-dev-render-on-client": "better-npm-run start-dev-render-on-client",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "dev-render-on-client": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev-render-on-client\"",
    "dev-fast": "npm run dev-render-on-client",
    "test": "jest BasketAddByCode.test.jsx",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "echo 1",
    "build-storybook2": "better-npm-run build-storybook",
    "lint-staged": "lint-staged"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "blueprints"
    ],
    "moduleNameMapper": {
      "^.+\\.styl$": "<rootDir>/webpack/stylesMock.js"
    },
    "transform": {
      "^.+\\.js*": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!d3-array|d3-scale|d3-shape|d3-time)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.story.js",
      "!**/*.mock.js",
      "!**/index.js",
      "!./webpack/**"
    ]
  },
  "moduleRoots": [
    "src"
  ],
  "betterScripts": {
    "start": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-test": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-dev-render-on-client": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "__DISABLE_SSR__": true
      }
    },
    "watch-client": {
      "command": "node ./bin/webpack.js",
      "env": {
        "UV_THREADPOOL_SIZE": 4,
        "NODE_PATH": "./src/"
      }
    },
    "build": {
      "command": "webpack --color --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "stats": {
      "command": "webpack --color --display-error-details --config webpack/prod.config.js --json > webpack-stats.json",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-storybook": {
      "command": "build-storybook -o static/__components",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@kadira/react-storybook-addon-info": "^3.4.0",
    "@kadira/storybook": "^2.35.3",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.1",
    "bson-objectid": "^1.1.5",
    "classnames": "^2.2.3",
    "compression": "^1.7.0",
    "concurrently": "^3.5.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.2",
    "debug": "^2.6.8",
    "envify": "^4.0.0",
    "enzyme": "^2.9.1",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "file-loader": "^0.11.1",
    "font-awesome-webpack": "^0.0.4",
    "happypack": "^2.2.1",
    "hoist-non-react-statics": "^2.2.0",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.1",
    "jest-cli": "^20.0.1",
    "memoizee": "^0.4.5",
    "moment": "^2.18.1",
    "piping": "^1.0.0-rc.4",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.5.10",
    "pure-render-decorator": "^1.2.1",
    "ramda": "^0.24.1",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-addons-perf": "^15.4.2",
    "react-cookie": "^0.4.6",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "https://github.com/Imater/react-redux/tarball/081965d68caea282d18f563f3eb9fbb88adcafd5",
    "react-router": "^3.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.2",
    "react-test-renderer": "^15.6.1",
    "react-tunnel": "^0.1.0",
